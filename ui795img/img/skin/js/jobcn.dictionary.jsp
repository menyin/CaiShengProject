	jobcn.Dictionary.JobFunction.getText=function(value,split){
		if(!value)return [];
		if(typeof(value)=="string")
		{
			if(value=="0")return [];
			split = split || ",";
			value = value.split(split);
		}
		var texts = [],
		tmp,
		item=(this.item[jobcn.Dictionary.lang]||this.item);
		for(var i=0;i< value.length;i++){
			tmp=value[i];
			if(tmp && tmp!="0" && (tmp = item[tmp]))texts.push(tmp);
		}
		return texts;
	};
