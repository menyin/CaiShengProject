jobcn.Dictionary={
	lang:"cn",
	getText:function(item,value,defaultText){
		var it=(item[this.lang]||item)[value];
		if(it){
			if(typeof(it)=="string")return it;
			else return it.text;
		}

		if(defaultText!=null){
			return defaultText;
		}else{
			return "";
		}
	},
	getOptions:function(item,defaultValue){
		var html=[];
		var t;
		var it=item[this.lang]||item;
		for(var s in it)
		{
			t=it[s];
			html.push('<option value="'+s+'"');
			if(defaultValue==s)html.push(' selected');
			html.push(">");
			html.push(t);
			html.push('</options>');
		}
		return html.join("");
	},
	add:function(key,value){
		if(typeof(log)=="function")log("jobcn.Dictionary","Load "+key);
		this[key]={
			item:value,
			getText:function(value,defaultText){
				return jobcn.Dictionary.getText(this.item,value,defaultText);
			},
			getOptions:function(defaultValue){
				return jobcn.Dictionary.getOptions(this.item,defaultValue);
			}
		};
	}
};

